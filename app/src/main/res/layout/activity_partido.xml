<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activities.PartidoActivity">


    <TextView
            android:id="@+id/tv_team1"
            android:layout_width="74dp"
            android:layout_height="20dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="parent"
            tools:layout_editor_absoluteY="320dp" app:layout_constraintStart_toEndOf="@+id/btn_Uno_team2"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.327"/>

    <Button
            android:id="@+id/btn_Uno_team2"
            android:layout_width="84dp"
            android:layout_height="36dp"
            android:text="+1"
            android:textSize="10sp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="24dp"
            app:layout_constraintEnd_toStartOf="@+id/imageView9" android:layout_marginRight="24dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/btn_Tres_team1"
            app:layout_constraintVertical_bias="0.187"/>

    <TextView
            android:id="@+id/tv_contTeam2"
            android:layout_width="62dp"
            android:layout_height="73dp"
            android:gravity="center"
            android:text="0"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:textSize="36sp" android:layout_marginBottom="148dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView9" android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp" app:layout_constraintHorizontal_bias="0.243"
    />

    <Button
            android:id="@+id/btn_Dos_team2"
            android:layout_width="82dp"
            android:layout_height="35dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:text="+2"
            android:textSize="10sp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/imageView9"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.772" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/btn_Uno_team2" app:layout_constraintVertical_bias="0.004"/>

    <Button
            android:id="@+id/btn_Tres_team2"
            android:layout_width="80dp"
            android:layout_height="35dp"
            android:text="+3"
            android:textSize="10sp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/imageView9"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.753" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/btn_Dos_team2" app:layout_constraintVertical_bias="0.017"/>

    <TextView
            android:id="@+id/tv_contTeam1"
            android:layout_width="63dp"
            android:layout_height="83dp"
            android:layout_marginTop="116dp"
            android:gravity="center"
            android:text="0"
            app:layout_constraintTop_toTopOf="parent" android:textSize="36sp"
            app:layout_constraintStart_toEndOf="@+id/imageView8" android:layout_marginStart="12dp"
            android:layout_marginLeft="12dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.056"/>

    <Button
            android:id="@+id/btn_Uno_team1"
            android:layout_width="87dp"
            android:layout_height="36dp"
            android:layout_marginStart="40dp"
            android:layout_marginLeft="40dp"
            android:text="+1"
            android:textSize="10sp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/tv_fecha"
            app:layout_constraintVertical_bias="0.076" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toStartOf="@+id/imageView8" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.809"/>

    <Button
            android:id="@+id/btn_Dos_team1"
            android:layout_width="85dp"
            android:layout_height="37dp"
            android:layout_marginStart="72dp"
            android:layout_marginLeft="72dp"
            android:text="+2"
            android:textSize="10sp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/btn_Uno_team1" app:layout_constraintVertical_bias="0.009"/>

    <Button
            android:id="@+id/btn_Tres_team1"
            android:layout_width="86dp"
            android:layout_height="33dp"
            android:text="+3"
            android:textSize="10sp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="80dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="80dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/btn_Dos_team1" app:layout_constraintVertical_bias="0.0"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/imageView8"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.0"/>

    <TextView
            android:id="@+id/tv_team2"
            android:layout_width="wrap_content"
            android:layout_height="16dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="96dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.476"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/imageView8"
            app:layout_constraintVertical_bias="0.0"/>

    <Button
            android:id="@+id/bt_finalizarPartido"
            android:layout_width="100dp"
            android:layout_height="46dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginBottom="40dp"
            android:text="End game"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.525"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/imageView9"
            app:layout_constraintVertical_bias="0.872"/>

    <TextView
            android:id="@+id/tv_fecha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="128dp"
            android:layout_marginLeft="128dp"
            android:layout_marginTop="48dp"
            android:text="TextView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="33dp" app:layout_constraintBottom_toTopOf="@+id/tv_team2"
            app:layout_constraintVertical_bias="0.0"/>

    <TextView
            android:id="@+id/tv_hora"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintVertical_bias="0.357" app:layout_constraintBottom_toTopOf="@+id/tv_team2"
            app:layout_constraintStart_toEndOf="@+id/tv_fecha" android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp" app:layout_constraintHorizontal_bias="0.093"/>
    <ImageView
            android:layout_width="80dp"
            android:layout_height="83dp" app:srcCompat="@drawable/equipo1"
            android:id="@+id/imageView8" android:layout_marginTop="116dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.015"
            app:layout_constraintStart_toEndOf="@+id/btn_Dos_team1" android:layout_marginStart="20dp"
            android:layout_marginLeft="20dp"/>
    <ImageView
            android:layout_width="86dp"
            android:layout_height="88dp" app:srcCompat="@drawable/equipo2" android:id="@+id/imageView9"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.546" android:layout_marginBottom="148dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/tv_team1"
            app:layout_constraintVertical_bias="1.0"/>

</androidx.constraintlayout.widget.ConstraintLayout>